<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#fafafa" >

    <LinearLayout
        android:id="@+id/category12"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background"
        android:orientation="horizontal" 
       >
        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="20dp"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/imagebutton_back"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:background="@drawable/toolbar_home_button"
                android:contentDescription="@string/desc" >
            </ImageButton>
        </FrameLayout>

        <TextView
            android:id="@+id/textview_titleshow"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/toolbar_title"
            android:textColor="#000000"
            android:textSize="16sp"
            android:typeface="sans"
            android:gravity="center_vertical"
            android:layout_gravity="center"
            android:layout_weight="1"/>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_gravity = "right"
            android:gravity = "right"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/fourshow"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="2dp"
                android:background="@drawable/toolbar_gallery_button"
                android:contentDescription="@string/desc"/>

            <ImageButton
                android:id="@+id/oneshow"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginRight="8dp"
                android:background="@drawable/toolbar_list_button"
                android:contentDescription="@string/desc" />

        </LinearLayout>

    </LinearLayout>
                
     <TableLayout
         android:id="@+id/category13"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/category14"
         android:layout_below="@+id/category12"
         android:layout_centerHorizontal="true"
         android:orientation="vertical" 
         android:gravity="center_vertical">
        <TableRow
            android:layout_width="260dp"
            android:gravity="center_horizontal" >

            <View
                android:layout_width="5dp"
                android:layout_height="110dp"
                android:gravity="left" />

            <ImageButton
                android:id="@+id/imagebutton1_1"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/mlb201010yj001"
                android:gravity="center"
                android:contentDescription="@string/desc" />

            <View
                android:layout_width="30dp"
                android:layout_height="110dp"
                android:gravity="left" />

            <ImageButton
                android:id="@+id/imagebutton1_2"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/mlb201010yj001"
                android:gravity="center" 
                android:contentDescription="@string/desc"/>

            <View
                android:layout_width="5dp"
                android:layout_height="110dp"
                android:gravity="left" />
        </TableRow>

        <TableRow
            android:layout_width="260dp"
            android:gravity="center_horizontal" >

            <View
                android:layout_width="5dp"
                android:layout_height="50dp"
                android:background="#fafafa"
                android:gravity="left" />

            <TextView
                android:id="@+id/textview1_1"
                android:layout_width="110dp"
                android:layout_height="50dp"
                android:text="@string/textview_content_temp">
            </TextView>

            <View
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:background="#fafafa"
                android:gravity="left" />

            <TextView
                android:id="@+id/textview1_2"
                android:layout_width="110dp"
                android:layout_height="50dp"
                android:text="@string/textview_content_temp">
            </TextView>

            <View
                android:layout_width="5dp"
                android:layout_height="50dp"
                android:background="#fafafa"
                android:gravity="left" />
        </TableRow>

        <View
            android:layout_width="260dp"
            android:layout_height="2dip"
            android:background="#fafafa" />

        <TableRow
            android:layout_width="260dp"
            android:gravity="center_horizontal" >

            <View
                android:layout_width="5dp"
                android:layout_height="110dp"
                android:gravity="left" />

            <ImageButton
                android:id="@+id/imagebutton2_1"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/mlb201010yj001"
                android:gravity="left" 
                android:contentDescription="@string/desc"/>

            <View
                android:layout_width="30dp"
                android:layout_height="110dp"
                android:gravity="left" />

            <ImageButton
                android:id="@+id/imagebutton2_2"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:background="@drawable/mlb201010yj001"
                android:gravity="center" 
                android:contentDescription="@string/desc"/>

            <View
                android:layout_width="5dp"
                android:layout_height="110dp"
                android:gravity="left" />
        </TableRow>

        <TableRow
            android:layout_width="260dp"
            android:gravity="center_horizontal" >

            <View
                android:layout_width="5dp"
                android:layout_height="50dp"
                android:background="#fafafa"
                android:gravity="left" />

            <TextView
                android:id="@+id/textview2_1"
                android:layout_width="110dp"
                android:layout_height="50dp"
                android:text="@string/textview_content_temp">
            </TextView>

            <View
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:background="#fafafa"
                android:gravity="left" />

            <TextView
                android:id="@+id/textview2_2"
                android:layout_width="110dp"
                android:layout_height="50dp"
                android:text="@string/textview_content_temp" >
            </TextView>

            <View
                android:layout_width="5dp"
                android:layout_height="50dp"
                android:background="#fafafa" />
        </TableRow>
    </TableLayout>

    <LinearLayout
        android:id="@+id/category14"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/black"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/button_preview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/pre1"
            android:gravity="right"
            android:contentDescription="@string/desc">
        </ImageButton>

        <ImageButton
            android:id="@+id/button_next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/next1"
            android:gravity="right"
            android:contentDescription="@string/desc" >
        </ImageButton>
    </LinearLayout>

</RelativeLayout>