<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:gravity="center" android:stretchColumns="5">
	<TableRow>
		<View android:layout_height="100px" android:layout_width="5px"
			android:gravity="left" android:background="#FFFFFFFF" />
		<ImageButton android:id="@+id/imagebutton1_1"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="center" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
		<ImageButton android:id="@+id/imagebutton1_2"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="center" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" />
		<ImageButton android:id="@+id/imagebutton1_3"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="right" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
	</TableRow>
	<View android:layout_height="2dip" android:background="#FFFFFFFF" />
	<TableRow>
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
		<ImageButton android:id="@+id/imagebutton2_1"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="left" android:background="@drawable/mlb201010yj001" />

		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
		<ImageButton android:id="@+id/imagebutton2_2"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="center" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" />
		<ImageButton android:id="@+id/imagebutton2_3"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="right" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />

	</TableRow>
	<View android:layout_height="2dip" android:background="#FFFFFFFF" />
	<TableRow>
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
		<ImageButton android:id="@+id/imagebutton3_1"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="left" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
		<ImageButton android:id="@+id/imagebutton3_2"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="center" android:background="@drawable/mlb201010yj001" />
		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" />
		<ImageButton android:id="@+id/imagebutton3_3"
			android:layout_width="100px" android:layout_height="100px"
			android:gravity="right" android:background="@drawable/mlb201010yj001" />

		<View android:layout_height="100px" android:layout_width="5px"
			android:background="#FFFFFFFF" android:gravity="left" />
	</TableRow>
	<LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:gravity="left"
		android:layout_weight="1">
		<Button android:id="@+id/button_preview" android:layout_width="80px" android:layout_height="80px"  
		 android:gravity="center"
			android:clickable="true" android:text="Preview" android:background="@drawable/forwardicon"
			android:layout_weight="1"></Button>
		<Button android:id="@+id/button_next" android:layout_width="80px" android:layout_height="80px" 
		 android:gravity="center"
			android:clickable="true" android:text="Next" android:layout_weight="1" android:background="@drawable/nexticon"></Button>
		<Button android:id="@+id/button_detail" android:layout_width="80px" android:layout_height="80px" 
			 android:gravity="center"
			android:clickable="true" android:text="Detail" android:layout_weight="1" android:background="@drawable/detailicon"></Button>
		<Button android:id="@+id/button_exit" android:layout_width="80px" android:layout_height="80px" 
			 android:gravity="center"
			android:clickable="true" android:text="EXIT" android:layout_weight="1" android:background="@drawable/exiticon"></Button>
	</LinearLayout>

</TableLayout> 
